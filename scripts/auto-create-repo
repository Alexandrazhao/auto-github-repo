#!/bin/bash
curl -H "Authorization: token $GITHUB_MY_APP_DEPLOYMENT_ACCESS_TOKEN" \
    --data '{"name":"auto-generate-repo2"}'\
    https://api.github.com/user/repos

#local user_slash_repo=$MY_REPO

if curl -fsS "https://api.github.com/repos/Alexandrazhao/auto-generate-repo2" >/dev/null; then
    printf '%s\n' "The GitHub repo repo2 exists." >&2
    return 0
  else
    printf '%s\n' "Error: no GitHub repo repo2 found." >&2
    return 1
  fi